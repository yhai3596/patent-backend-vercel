{
  "version": 2,
  "name": "patent-backend",
  "framework": null,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node@2.0.0"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "JWT_SECRET": "patent-backend-secret-key-2024-vercel"
  }
}
